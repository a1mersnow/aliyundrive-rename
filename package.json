{
  "name": "aliyundrive-rename",
  "type": "module",
  "version": "0.2.2",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "Batch rename files of aliyundrive.",
  "author": "a1mersnow",
  "repository": "https://github.com/a1mersnow/aliyundrive-rename",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^10.6.1",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.3",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/ion": "^1.1.12",
    "@types/node": "^20.9.0",
    "@unocss/eslint-config": "^0.56.5",
    "@unocss/eslint-plugin": "^0.56.5",
    "@unocss/reset": "^0.56.5",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.53.0",
    "eslint-define-config": "^1.24.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.10.5",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.4",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-monkey": "^3.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
